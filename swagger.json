            "type": "string"
          },
          {
            "name": "status",
            "in": "formData",
            "description": "Updated status of the person",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "405": {
            "description": "Invalid input"
          }
        },
        "security": [
          {
            "personstore_auth": [
              "write:person",
              "read:person"
            ]
          }
        ]
      },
      "delete": {
        "tags": [
          "persona"
        ],
        "summary": "Deletes a person",
